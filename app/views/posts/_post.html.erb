<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Subtitle:</strong>
    <%= post.subtitle %>
  </p>

  <p>
    <strong>Bich text:</strong>
    <%= post.bich_text %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user_id %>
  </p>

  <p>
    <strong>Comments:</strong>
    <%= post.comments %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= post.category %>
  </p>

</div>
